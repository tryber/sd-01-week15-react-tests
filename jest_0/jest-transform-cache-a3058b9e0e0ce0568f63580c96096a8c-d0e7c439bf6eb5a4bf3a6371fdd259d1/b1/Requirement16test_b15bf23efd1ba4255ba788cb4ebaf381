const isPokemonFavoriteById = {
  1: true,
  2: false,
  3: false,
  4: false
};

const testPossiblePoker = (poker, pokerId) => {
  const _render = (0, _react2.render)(_react.default.createElement(_reactRouterDom.MemoryRouter, {
    initialEntries: ['/'],
    __source: {
      fileName: _jsxFileName,
      lineNumber: 123
    },
    __self: void 0
  }, _react.default.createElement(_components.Pokedex, {
    pokemons: pokemons,
    isPokemonFavoriteById: isPokemonFavoriteById,
    __source: {
      fileName: _jsxFileName,
      lineNumber: 124
    },
    __self: void 0
  }))),
        getByText = _render.getByText,
        getByAltText = _render.getByAltText,
        queryByAltText = _render.queryByAltText;

  const btnNextPokemon = getByText('Próximo pokémon');
  poker.forEach(pokemon => {
    if (pokerId[pokemon.id]) {
      const imgStar = getByAltText(`${pokemon.name} is marked as favorite`);
      expect(imgStar).toBeInTheDocument();
      expect(imgStar.src).toBe('http://localhost/star-icon.svg');
    } else {
      expect(queryByAltText(`${pokemon.name} is marked as favorite`)).not.toBeInTheDocument();
    }

    _react2.fireEvent.click(btnNextPokemon);
  });
};

describe('16 → Pokémons favoritados devem exibir um ícone de uma estrela', () => {
  test('Pokémons favoritados devem exibir um ícone de uma estrela', () => {
    testPossiblePoker(pokemons, isPokemonFavoriteById);
  });
});
