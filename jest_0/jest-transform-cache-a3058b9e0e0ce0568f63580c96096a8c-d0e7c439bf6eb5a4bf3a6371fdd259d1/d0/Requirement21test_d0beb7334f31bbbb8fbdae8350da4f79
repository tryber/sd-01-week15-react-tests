"use strict";

var _interopRequireDefault = require("/home/doug/projetos-trybe/sd-01-week15-react-tests/node_modules/babel-preset-react-app/node_modules/@babel/runtime/helpers/interopRequireDefault");

var _react = _interopRequireDefault(require("react"));

var _reactRouterDom = require("react-router-dom");

var _react2 = require("@testing-library/react");

var _About = _interopRequireDefault(require("../components/About"));

var _jsxFileName = "/home/doug/projetos-trybe/sd-01-week15-react-tests/src/tests/Requirement-21.test.js";
describe('Exigência → 21', () => {
  test('A página "About" deve exibir informações sobre a Pokédex', () => {
    const _render = (0, _react2.render)(_react.default.createElement(_reactRouterDom.MemoryRouter, {
      initialEntries: ['/'],
      __source: {
        fileName: _jsxFileName,
        lineNumber: 9
      },
      __self: void 0
    }, _react.default.createElement(_About.default, {
      __source: {
        fileName: _jsxFileName,
        lineNumber: 10
      },
      __self: void 0
    }))),
          getByText = _render.getByText;

    const link = 'https://cdn.bulbagarden.net/upload/thumb/8/86/Gen_I_Pok%C3%A9dex.png/800px-Gen_I_Pok%C3%A9dex.png';
    const text = 'About Pokédex';
    expect(getByText(text)).toBeInTheDocument();
    expect(getByText(text).tagName).toBe('H2');
    expect(getByText(text).nextSibling.firstChild.tagName).toBe('P');
    expect(getByText(text).nextSibling.firstChild.nextSibling.tagName).toBe('P');
    expect(getByText(text).nextSibling.firstChild.nextSibling.nextSibling.src).toBe(link);
  });
});
